<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<context:annotation-config/>
	
	<bean id="profile" class="com.practice.self.project.spring.annotations.Profile">
	</bean>
	
	<bean id="employee1" class="com.practice.self.project.spring.annotations.Employee">
		<property name="employeeId" value="12345" />
		<property name="employeeName" value="John Doe" />
	</bean>
	
	<bean id="employee2" class="com.practice.self.project.spring.annotations.Employee">
		<property name="employeeId" value="23456" />
		<property name="employeeName" value="Sarah Parker" />
	</bean>
	
	<bean id="chatBot" class="com.practice.self.project.spring.annotations.ChatBot">
	</bean>
	<bean id="chatEngine" class="com.practice.self.project.spring.annotations.ChatEngine">
	</bean>
	<bean id="emoticons" class="com.practice.self.project.spring.annotations.Emoticons">
	</bean>
	
	<bean id="gettingStarted" class="com.practice.self.project.spring.xml.GettingStarted">
		<property name="message" value="Getting Started..." />
	</bean>
</beans>